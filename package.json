{
  "name": "systemic",
  "version": "3.0.0",
  "description": "A minimal dependency injection library for node",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "lint": "eslint .",
    "istanbul": "istanbul cover --report html --report lcov _mocha",
    "codeclimate": "(test -e .codeclimate && npm run istanbul && source .codeclimate) || echo skipping codeclimate",
    "nsp": "nsp check",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run codeclimate && npm run nsp"
  },
  "keywords": [
    "dependency",
    "injection",
    "context",
    "inversion of control",
    "graceful",
    "start up",
    "shutdown",
    "ioc",
    "boot"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.0.0",
    "eslint-config-imperative": "^2.0.0",
    "eslint-plugin-imperative": "^2.0.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1"
  },
  "dependencies": {
    "async": "^2.6.0",
    "chance": "^1.0.16",
    "debug": "^4.0.0",
    "lodash.assign": "^4.2.0",
    "lodash.defaults": "^4.2.0",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.intersection": "^4.4.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.map": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.toarray": "^4.4.0",
    "require-all": "^2.1.0",
    "toposort-class": "^1.0.1",
    "uuid": "^3.2.1"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/systemic.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/systemic/issues"
  },
  "homepage": "https://guidesmiths.github.io/systemic/"
}
